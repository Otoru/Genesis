<include>
  <!-- Basic Settings -->
  <X-PRE-PROCESS cmd="set" data="default_password=1234"/>
  <X-PRE-PROCESS cmd="set" data="console_loglevel=info"/>
  <X-PRE-PROCESS cmd="set" data="default_country=US"/>

  <!-- Domain & IP -->
  <X-PRE-PROCESS cmd="set" data="force_local_ip_v4=192.168.64.2"/>
  <X-PRE-PROCESS cmd="set" data="local_ip_v4=$${force_local_ip_v4}"/>
  <X-PRE-PROCESS cmd="set" data="domain=$${local_ip_v4}"/>
  <X-PRE-PROCESS cmd="set" data="domain_name=$${domain}"/>

  <!-- Directories & Sounds -->
  <X-PRE-PROCESS cmd="set" data="sound_prefix=$${sounds_dir}/en/us/callie"/>
  <X-PRE-PROCESS cmd="set" data="hold_music=local_stream://moh"/>

  <!-- SIP Ports (References for profiles) -->
  <X-PRE-PROCESS cmd="set" data="internal_sip_port=5060"/>

  <!-- Codecs -->
  <X-PRE-PROCESS cmd="set" data="global_codec_prefs=OPUS,G722,PCMU,PCMA,H264,VP8"/>
</include>
